<div class="profile-container">
  
  <%- include('../partials/acct-nav.ejs') %>

  <main class="profile-display">
    <div class="profile-account">
      <p class="profile-title">Comments</p>
    </div>

    <section class="profile-content">
      <% favorites.forEach(favorite => { %>
        <p><a href="/listings/<%= favorite.page_id) %>"><%= favorite.title %></a></p>
        <p>Posted by - u/<%= favorite.author %></p>
      <% }) %>
    </section>
  </main>
</div>